{
  "name": "@binaryknights/sass-mixins",
  "version": "0.0.1",
  "description": "Sass mixins for generating boilerblate css and grid frameworks",
  "repository": {
    "type": "git",
    "url": "https://github.com/binaryknights/sass-mixins"
  },
  "bugs": {
    "url": "https://github.com/binaryknights/sass-mixins/issues"
  },
  "homepage": "https://github.com/binaryknights/sass-mixins",
  "author": "",
  "keywords": [
    "sass",
    "css",
    "boilerplate"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "css": "npm-run-all --parallel css-lint* css-compile* --sequential css-prefix* css-minify*",
    "css-compile": "sass test/scss/style.scss test/css/style.css",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"test/css/*.css\" \"!test/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output test/css/style.min.css test/css/style.css",
    "watch-css": "nodemon --ignore dist/ -e scss -x \"npm run css\""
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "clean-css-cli": "^4.3.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "sass": "^1.26.5",
    "stylelint": "^13.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 49",
    "Firefox >= 38",
    "Edge >= 16",
    "iOS >= 9",
    "Safari >= 9.1",
    "Android >= 81",
    "Opera >= 30"
  ]
}
